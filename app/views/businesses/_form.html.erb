<%= render 'layouts/errors', :object => @business %>

<%= form_for @business do |f| %>
  <%= f.label :Name %>
  <%= f.text_field :biz_name, placeholder: 'Pok Pok' %>

  <%= f.label :Type %>
  <%= f.text_field :biz_type, placeholder: 'Restaurant'  %>

  <%= f.label :Address %>
  <%= f.text_field :biz_address, placeholder: '123 Main St'  %>

  <%= f.label :'Phone number' %>
  <%= f.text_field :biz_phone, placeholder: '555-555-5555'  %>

  <%= f.label :Contact %>
  <%= f.text_field :biz_contact, placeholder: 'Jane Doe'  %>

  <%= f.label :'Business Number' %>
  <%= f.text_field :biz_number, placeholder: '#4'  %>

  <%= f.submit %>
<% end %>
